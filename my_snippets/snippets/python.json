{
  "logfmt": {
    "prefix": "logfmt",
    "body": "\"%(asctime)s - %(name)s - %(levelname)s - %(message)s\""
  },
  "logbsc": {
    "prefix": "logbsc",
    "body": "import logging\n\nlogging.basicConfig(\n    format=\"%(asctime)s - %(name)s - %(levelname)s - %(message)s\")"
  },
  "matplotrc": {
    "prefix": "pltr",
    "body": "plt.rcParams[\"font.sans-serif\"] = [\"Adobe Heiti Std\"]\nplt.rcParams[\"axes.unicode_minus\"] = False"
  }
}
